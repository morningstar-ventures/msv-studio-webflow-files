{
  "trailingComma": "es5",
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 100,
  "quoteProps": "as-needed",
  "tabWidth": 2,
  "useTabs": false,
  "importOrder": ["^[^.]", "", "^.*(?<!\\.(mp3|scss))$", "", "\\.mp3$", "", "\\.scss$"],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "importOrderMergeDuplicateImports": true
}
